{
  "extends": "./node_modules/gts/tsconfig-google.json",  
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "build",
    "rootDir": ".",
    "strict": true,
    "target": "es2021",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "composite": true,
    "lib": [
      "DOM"
    ]
  },
  "include": [
    "src/*.ts"
  ]
}

